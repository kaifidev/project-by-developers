    border: 0;
    padding: 24px 32px;
    box-sizing: border-box;
    flex-grow: 1;
}

#onetrust-pc-sdk .pc-header .pc-logo-container {
    display: none;
}

#onetrust-consent-sdk #onetrust-pc-sdk #vendors-list .navigation-container #vendors-list-title {
    font-size: 16px;
    font-weight: 600;
    color: #222325;
}

#onetrust-pc-sdk .pc-header .pc-title-container {
    float: none;
    margin: 0;
}

#onetrust-consent-sdk #onetrust-pc-sdk .pc-header .pc-title-container #pc-title {
    margin: 0;
    font-weight: bold;
    font-size: 20px;
    color: #222325;
}

#onetrust-consent-sdk #onetrust-pc-sdk .ot-sdk-container #pc-tab-description,
#onetrust-consent-sdk #onetrust-pc-sdk .ot-sdk-container .description-container {
    padding-top: 0;
    outline: 0;
}

